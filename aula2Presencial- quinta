// Criar 3 variáveis com tipos Objeto Number para guardar os valores:
//questao1
var number = new  Number (300);
console.log(number);

var number = new  Number (999);
console.log(number);

var number = new  Number (21);
console.log(number);

//Exibir no console os valores de 4 propriedades do tipo objeto Number.
//questao2
var number = new  Number (800);
console.log(Number.EPSILON);
console.log(Number.MAX_SAFE_INTEGER);
console.log(Number.MAX_VALUE);
console.log(Number.MIN_VALUE);

//Criar 1 variável para receber o valor 150:
//questao3
var numero = 150;
console.log(numero);
console.log(numero.toString())
